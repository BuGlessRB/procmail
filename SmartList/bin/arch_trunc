#! /bin/sh
:
#$Id: arch_trunc,v 1.7 1994/07/19 14:44:59 berg Exp $

echo=echo		# /bin/echo
ls=ls			# /bin/ls
sed=sed			# /bin/sed
rm=rm			# /bin/rm

if cd archive/latest
then
  case "$ARCHIVE" in	# do not start removing every time, to decrease load
     *[248])
	$rm -f _dummy_ \
	 `$ls -t | $sed -e '/[^0-9]/ d' | $sed -e '1,'$archive_hist' d'` ;;
  esac
else
  $echo "Don't start this script directly, it is used in rc.request"
  exit 64
fi

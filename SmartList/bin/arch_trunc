#! /bin/sh
:
#$Id: arch_trunc,v 1.5 1994/05/26 14:11:07 berg Exp $

echo=echo		# /bin/echo
ls=ls			# /bin/ls
sed=sed			# /bin/sed
rm=rm			# /bin/rm

if cd archive && cd latest
then
  case "$LASTFOLDER" in # do not start removing every time, to decrease load
     *[248])
	$rm -f _dummy_ \
	 `$ls -t | $sed -e '1,'$archive_hist' d' -e '/[^0-9]/ d' ` ;;
  esac
else
  $echo "Don't start this script directly, it is used in rc.request"
  exit 64
fi

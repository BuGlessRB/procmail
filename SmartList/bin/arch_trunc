#! /bin/sh
: &&O= || exec /bin/sh $0 $argv:q # we're in a csh, feed myself to sh
#$Id: arch_trunc,v 1.1 1993/01/15 14:40:43 berg Exp $

echo=echo		# /bin/echo
ls=ls			# /bin/ls
sed=sed			# /bin/sed
rm=rm			# /bin/rm

if cd archive && cd latest
then
  case "$1" in		# do not start removing every time, to decrease load
     *[248])
	$rm -f `$ls -t | $sed -e '1,'$archive_hist' d' -e '[^0-9] d' ` ;;
  esac

  $echo "$1"
else
  $echo "Don't start this script directly, it is used in rc.request"
  exit 64
fi
